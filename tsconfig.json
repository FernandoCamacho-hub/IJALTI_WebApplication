{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
    
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/api/hello.tsx", "config/db.jsx", "components/userForm.tsx", "pages/index.tsx", "pages/api/clientes/index.js", "components/[profile].js", "components/[profile].js", "components/profile.js", "components/profile.js"],
  "exclude": ["node_modules"]
}
